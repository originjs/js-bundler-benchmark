{
  "name": "js-bundler-demo-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:esbuild": "node esbuild.mjs --watch",
    "start:farm": "farm start --config farm.config.js",
    "start:parcel": "parcel index.html --dist-dir dist-parcel",
    "start:rollup": "rollup -c rollup.dev.config.js",
    "start:rsbuild": "rsbuild dev --config rsbuild.config.js",
    "start:rspack": "rspack serve --config rspack.config.mjs",
    "start:snowpack": "snowpack dev --config snowpack.config.mjs",
    "start:vite": "vite",
    "start:webpack": "webpack serve --config webpack.config.mjs --mode development",
    "build:esbuild": "node esbuild.mjs",
    "build:farm": "cross-env IS_BUILD=1 farm build --config farm.config.js",
    "build:parcel": "cross-env NODE_ENV=production parcel build index.html --dist-dir dist-parcel --no-source-maps --no-cache",
    "build:rollup": "rollup -c rollup.config.js",
    "build:rsbuild": "rsbuild build --config rsbuild.config.js",
    "build:rspack": "rspack build --config rspack.config.mjs",
    "build:vite": "run-p type-check \"build-only {@}\" --",
    "build:webpack": "webpack build --config webpack.config.mjs --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@farmfe/cli": "^0.6.0",
    "@farmfe/core": "^0.16.0",
    "@farmfe/js-plugin-less": "1.3.0",
    "@farmfe/js-plugin-vue": "3.2.0",
    "@parcel/transformer-vue": "2.11.0",
    "@playwright/test": "^1.40.1",
    "@rsbuild/core": "^0.3.7",
    "@rsbuild/plugin-vue": "^0.3.7",
    "@rspack/cli": "^0.5.2",
    "@rspack/core": "^0.5.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-vue": "^2.6.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "esbuild": "^0.19.12",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-vue": "^0.2.4",
    "esbuild-plugin-vue3": "^0.4.2",
    "esbuild-vue": "^1.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "html-webpack-plugin": "^5.6.0",
    "jsdom": "^23.0.1",
    "less": "^4.2.0",
    "less-loader": "^12.1.0",
    "npm-run-all2": "^6.1.1",
    "parcel": "^2.11.0",
    "prettier": "^3.0.3",
    "rollup": "^4.9.6",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "snowpack": "^3.8.8",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^1.8.25",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
