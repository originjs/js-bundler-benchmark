{
  "name": "js-bundler-benchmark",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "benchmark": "node packages/tools/benchmark.mjs",
    "benchmark:react-demo": "node packages/tools/benchmark.mjs -p=react-demo",
    "benchmark:dev": "node packages/tools/benchmark.mjs benchmark.mjs -t=dev",
    "benchmark:build": "node packages/tools/benchmark.mjs benchmark.mjs -t=build"
  },
  "// parcel configs": {
    "// note with resolve.extensions": "parcel doesn't provide a way to specify it",
    "// note with sourcemaps": "parcel includes sourcesContent field."
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.15"
  },
  "alias": {
    "process": false
  }
}
